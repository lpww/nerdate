# postgres config (read by docker when spinning up db)
export POSTGRES_HOST=localhost
export POSTGRES_DB=nerdate
export POSTGRES_PASSWORD=<yourpasswordhere>
export POSTGRES_USER=nerdate

# nerdate db data source name
export NERDATE_DB_DSN="postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}/${POSTGRES_DB}?sslmode=disable"
